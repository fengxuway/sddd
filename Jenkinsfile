pipeline {
  agent any
  stages {
    stage('qq') {
      parallel {
        stage('qq') {
          steps {
            echo '454'
            sh 'll'
          }
        }

        stage('error') {
          steps {
            echo 'fadsdf'
            sh 'ls -l'
          }
        }

      }
    }

    stage('dsfdsaf') {
      steps {
        echo 'dfsaf'
      }
    }

    stage('sss') {
      steps {
        echo 's'
      }
    }

  }
}